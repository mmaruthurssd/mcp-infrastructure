{
  "templateType": "mcp-server",
  "templateVersion": "1.0.0",
  "name": "documentation-generator-mcp-server-template",
  "displayName": "Documentation Generator MCP",
  "description": "Automated API documentation, changelogs, and diagrams for TypeScript projects",
  "author": "Medical Practice Workspace",
  "created": "2025-11-03",
  "installation": {
    "templateStoragePath": "templates-and-patterns/mcp-server-templates/templates/documentation-generator-mcp-server-template",
    "workingInstancePath": "local-instances/mcp-servers/documentation-generator-mcp-server",
    "requiresNodeModules": true,
    "requiresBuild": true,
    "buildCommand": "npm run build",
    "buildOutputDir": "dist"
  },
  "mcpConfiguration": {
    "required": true,
    "serverName": "documentation-generator-mcp",
    "command": "node",
    "entryPoint": "dist/server.js",
    "environmentVariables": {}
  },
  "prerequisites": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0",
    "tools": ["git"]
  },
  "documentation": {
    "readme": "README.md",
    "installation": "INSTALL-INSTRUCTIONS.md"
  },
  "aiInstructions": {
    "detectionFile": "TEMPLATE-INFO.json",
    "installationGuide": "INSTALL-INSTRUCTIONS.md",
    "autoInstallScript": "install.sh",
    "configurationScript": "configure-mcp.sh"
  }
}
