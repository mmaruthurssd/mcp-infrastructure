{
  "name": "workspace-health-dashboard-mcp-server",
  "version": "1.0.0",
  "description": "Unified health dashboard providing single pane of glass for workspace health, MCP status, autonomous resolution metrics, and system alerts",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('dist/index.js', '755')\"",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build"
  },
  "bin": {
    "workspace-health-dashboard-mcp-server": "./dist/index.js"
  },
  "keywords": [
    "mcp",
    "health",
    "dashboard",
    "monitoring",
    "workspace",
    "autonomous",
    "metrics",
    "alerts"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
